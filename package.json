    "react-dom": "^16.8.6",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "devDependencies": {
    "@types/compression": "0.0.36",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "@types/next": "^8.0.4",
    "@types/node": "^11.13.7",
    "@types/react": "^16.8.14",
    "cross-env": "^5.2.0",
    "prettier": "^1.17.0",
    "ts-node": "^8.1.0",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.16.0",
