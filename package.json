{
  "name": "my-book-list",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node --project tsconfig.server.json src/server/index.ts",
    "build": "rm -rf src/.next && next build src && tsc && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node src/.next",
    "lint": "prettier --check \"src/**/*.{ts,tsx}\" && tslint --project tsconfig.json && tslint --project tsconfig.server.json",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\" && tslint --project tsconfig.json --fix && tslint --project tsconfig.server.json --fix",
    "test": ""
  },
  "dependencies": {
    "@material-ui/core": "^4.0.1",
    "@material-ui/icons": "^4.0.1",
    "@material-ui/styles": "^4.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "next": "^8.1.0",
    "next-helpers": "^1.0.0",
    "progress-bar-webpack-plugin": "^1.12.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-easy-swipe": "0.0.17",
    "react-intl": "^2.9.0",
    "styled-jsx": "^3.2.1",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.8",
    "@types/compression": "0.0.36",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.13",
    "@types/next": "^8.0.5",
    "@types/node": "^12.0.4",
    "@types/react": "^16.8.19",
    "@types/react-intl": "^2.3.17",
    "@types/styled-jsx": "^2.2.8",
    "prettier": "^1.17.1",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.1"
  }
}
